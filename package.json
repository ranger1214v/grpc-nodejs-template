{
    "name": "grpc-nodejs-template",
    "version": "1.0.0",
    "description": "grpc-nodejs-template",
    "scripts": {
        "protoc-gen": "./node_modules/.bin/grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto  --ts_proto_out=./src/protos --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false --proto_path=./src/protos ./src/protos/action.proto",
        "build": "tsc && copyfiles -u 1 src/protos/** lib/src/",
        "start:server": "node lib/server.js",
        "start:client": "node lib/client.js",
        "copy": "copyfiles -u 1 src/protos/** lib/src/"
    },
    "author": "",
    "license": "Apache-2.0",
    "main": "lib/index.js",
    "dependencies": {
        "@grpc/grpc-js": "^1.7.1",
        "@grpc/proto-loader": "^0.7.3",
        "nice-grpc-common": "^2.0.0",
        "nice-grpc": "^2.0.0",
        "body-parser": "^1.19.0",
        "connect-timeout": "1.9.0",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "lodash": "^4.17.20",
        "long": "^5.2.0",
        "mime-types": "2.1.34",
        "minimist": "1.2.6",
        "moment": "2.24.0",
        "moment-timezone": "0.5.27",
        "prompts": "2.4.2",
        "protobufjs": "^7.1.2",
        "request": "2.88.2",
        "request-promise": "4.2.5",
        "rxjs": "~6.6.0",
        "ix": "5.0.0",
        "uuid": "3.3.3"
    },
    "devDependencies": {
        "@types/connect-timeout": "0.0.34",
        "@types/cors": "^2.8.10",
        "@types/lodash": "^4.14.168",
        "@types/mime-types": "^2.1.1",
        "@types/minimist": "1.2.2",
        "@types/moment-timezone": "0.5.12",
        "@types/node": "^13.13.42",
        "@types/prompts": "2.0.14",
        "@types/request-promise": "4.1.45",
        "@types/uuid": "3.4.6",
        "copyfiles": "2.4.1",
        "grpc-tools": "^1.11.2",
        "ts-proto": "^1.126.1",
        "typescript": "^4.8.0"
    }
}