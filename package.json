{
    "name": "grpc-nodejs-template",
    "version": "1.0.0",
    "description": "grpc-nodejs-template",
    "scripts": {
        "grpc-gen": "protoc --plugin='protoc-gen-ts=./node_modules/.bin/protoc-gen-ts_proto' --ts_out='./' ./src/protos/action.proto",
        "build": "tsc && copyfiles -u 1 src/protos/** lib/src/",
        "start:server": "node lib/server.js",
        "start:client": "node lib/client.js",
        "copy": "copyfiles -u 1 src/protos/** lib/src/"
    },
    "author": "",
    "license": "Apache-2.0",
    "main": "lib/index.js",
    "dependencies": {
        "ts-proto": "1.112.1",
        "@grpc/proto-loader": "0.6.12",
        "@grpc/grpc-js": "1.6.7",
        "rxjs": "~6.6.0",
        "grpc": "1.24.11",
        "google-protobuf": "3.20.1",
        "grpc-web": "1.3.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "lodash": "^4.17.20",
        "moment": "2.24.0",
        "moment-timezone": "0.5.27",
        "request": "2.88.2",
        "request-promise": "4.2.5",
        "uuid": "3.3.3",
        "connect-timeout": "1.9.0",
        "mime-types": "2.1.34",
        "minimist": "1.2.6",
        "prompts": "2.4.2"
    },
    "devDependencies": {
        "@types/connect-timeout": "0.0.34",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/lodash": "^4.14.168",
        "@types/mime-types": "^2.1.1",
        "@types/moment-timezone": "0.5.12",
        "@types/node": "^13.13.42",
        "@types/request-promise": "4.1.45",
        "@types/uuid": "3.4.6",
        "@types/google-protobuf": "3.15.6",
        "@types/minimist": "1.2.2",
        "@types/prompts": "2.0.14",
        "typescript": "^3.9.9",
        "copyfiles": "2.4.1"
    }
}